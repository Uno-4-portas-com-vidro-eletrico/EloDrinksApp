{
  "expo": {
    "name": "EloDrinksApp",
    "slug": "EloDrinksApp",
    "version": "1.0.0",
    "scheme": "com.uno4portas.EloDrinksApp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E0CEAA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uno4portas.EloDrinksApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#E0CEAA"
      },
      "package": "com.uno4portas.EloDrinksApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fa264bfd-c6e9-4e98-ae0b-ee37fa12e1b5"
      }
    },
    "owner": "elpandadlmx",
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#E0CEAA",
          "image": "./assets/icon.png",
          "resizeMode": "cover",
          "imageWidth": 240
        }
      ]
    ]
  }
}